@{
}
@model slistvm
<a href="@Url.Action("index","home")" class="btn btn-info">Back</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
            <th scope="col">Subject</th>
            <th scope="col">Teacher</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.melumatlar)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.SurName</td>
                <td>@item.Subject</td>
                <td>@item.Teacher</td>
               
            </tr>
        }

    </tbody>
</table>