@{
}
@section css{
    <link rel="stylesheet" href="~/Front/css/login.css" />
}
<h3>Login</h3>

<form class="all" action="login" method="post">
    <div class="uname">
        <label for="uname">Name</label>
        <input type="text" class="uname" id="uname" aria-describedby="emailHelp" placeholder="Enter name" name="name">
    </div>

    <div class="password">
        <label for="password">Password</label>
        <input type="password" class="password" id="password" placeholder="Password" name="pass">
    </div>

    <div class="btns">
        <button type="submit" class="btn btn-primary">Login</button>
      
    </div>
</form>

